{
  "name": "bookgig-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "azure-storage": "^2.2.0",
    "babel-cli": "^6.24.1",
    "express": "^4.15.3",
    "parse-dashboard": "^1.0.28",
    "parse-react": "^0.5.2",
    "parse-server": "^2.4.2",
    "parse-server-azure-push": "^1.0.2",
    "parse-server-azure-storage": "^1.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "immutable": "^3.8.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7",
    "seamless-immutable": "^7.1.2"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "server": "babel-node ./parse/index.js",
    "mongo": "mongod --dbpath ./mongodb",
    "dashboard": "parse-dashboard --appId bookgig --masterKey bookgig --serverURL 'http://localhost:1337/parse' --appName bookgig",
    "server-all": "npm-run-all -p mongo server dashboard",
    "start": "npm-run-all -p server mongo dashboard start-react",
    "start-react": "react-scripts start",
    "build-server": "babel -d ./build ./parse -s",
    "build": "yarn clean && yarn build-server && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
